class arthematic:
    def __init__(self,a,b):
        self.a=a
        self.b=b
    def mul(self):
        return self.a*self.b
    def sub(self):
        return self.a-self.b
    def add(self):
        return self.a+self.b
    def div(self):
        return self.a/self.b
    def mod(self):
        return self.a%self.b
    def power(self):
        return self.a**self.b
m=arthematic(2,3)
print(m.add())
print(m.sub())
print(m.mul())
print(m.div())
print(m.mod())
print(m.power())
