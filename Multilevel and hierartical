class dad():
    def phone(self):
        print("Allowed to use phone")
class son1(dad):
    def study(self):
        print("For study Purpose need phone")
class son2(dad):
    def study(self):
        print("For study Purpose need phone")
class son3(dad):
    def not_study(self):
        print("For not intrested")
s1=son1()
s1.phone()
s2=son2()
s2.phone()
s3=son3()
s3.not_study()